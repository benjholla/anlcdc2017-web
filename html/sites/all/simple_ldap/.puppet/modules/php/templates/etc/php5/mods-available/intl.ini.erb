; configuration for php Internationalisation module
; priority=20
extension=intl.so
<% if @default_locale -%>
intl.default_locale=<%= @default_locale %>
<% end -%>
<% if @error_level -%>
intl.error_level=<%= @error_level %>
<% end -%>
<% if @use_exceptions -%>
intl.use_exceptions=<%= @use_exceptions %>
<% end -%>
